    "def ymd2days(yyyy,mm,dd):\n",
    "    return(yyyy*360+mm*30+dd)\n",
    "\n",
    "def calHol(enter,today):\n",
    "    enter=ymd2days(2000,10,10)\n",
    "    today=ymd2days(2000,10,14)\n",
    "    return((today-enter)//14)\n",
    "\n",
    "enter=ymd2days(2000,10,10)\n",
    "today=ymd2days(2000,10,24)\n",
    "\n",
    "calHol(enter,today)\n"
   ]
  }
